{
  "name": "ar_gpio",
  "version": "0.1.0",
  "description": "GPIO Pin control in JS using the gpiochip character device.",
  "main": "dist/ar_gpio.js",
  "scripts": {
    "clean": "rm -R target",
    "build": "cargo build --release && " +
             "rm dist/ar_gpio.js && rm dist/ar_gpio.so && "
             "mv target/release/libar_gpio.so dist/ar_gpio.so && " +
             "cp src/ar_gpio.js dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shmishtopher/AR_GPIO.git"
  },
  "keywords": [
    "anderson",
    "robotics",
    "gpio",
    "pin"
  ],
  "author": "Shmish <shmish90@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shmishtopher/AR_GPIO/issues"
  },
  "homepage": "https://github.com/shmishtopher/AR_GPIO#readme",
  "dependencies": {
    "fastcall": "^0.2.6"
  }
}
